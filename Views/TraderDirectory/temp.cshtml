@{
    ViewData["Title"] = "Remove Trader";
}

<div class="container py-4">
    <div class="card shadow-sm">
        <div class="card-body">
            <h3 class="card-title">Remove Trader</h3>
            <hr />

            <form asp-action="RemoveTraderByEmail" method="post">
                <button type="submit" class="btn btn-danger">
                    <i class="fas fa-user-times me-1"></i> Remove Trader
                </button>
            </form>
        </div>
    </div>
</div>
